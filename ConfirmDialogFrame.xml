<Ui xmlns="http://www.blizzard.com/wow/ui/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	
	<Frame name="StriLi_ConfirmDialogFrame_Template" virtual="true" toplevel="true"
      movable="false" enableMouse="true" hidden="true">
		<Size>
			<AbsDimension x="430" y="100"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Backdrop tile="true"
        bgFile="Interface\DialogFrame\UI-DialogBox-Background"
        edgeFile="Interface\DialogFrame\UI-DialogBox-Border">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer>
				<FontString name="$parent_FontString" inherits="GameFontNormal" text="StriLi">
					<Anchors>
						<Anchor point="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parent_ConfirmButton" inherits="UIPanelButtonTemplate" text="Yes">
				<Size>
					<AbsDimension x="60" y="20"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="-40" y="-30"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
			<Button name="$parent_CancleButton" inherits="UIPanelButtonTemplate" text="Cancle">
				<Size>
					<AbsDimension x="60" y="20"/>
				</Size>
				<Anchors>
					<Anchor point="CENTER">
						<Offset>
							<AbsDimension x="40" y="-30"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Button>
		</Frames>
	</Frame>
</Ui>
