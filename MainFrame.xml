<Ui xmlns="http://www.blizzard.com/wow/ui/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	
	<Frame name="StriLi_ListTemplate" virtual="true">
		<Size>
			<AbsDimension x="91" y="470"/>
		</Size>
		<Backdrop
		edgeFile="Interface/Tooltips/UI-Tooltip-Border">
			<BackgroundInsets>
				<AbsInset left="30" right="30" top="30" bottom="30"/>
			</BackgroundInsets>
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
		</Backdrop>
	</Frame>
	
	<Frame name="StriLi_MainFrame" toplevel="true" frameStrata="DIALOG" 
      movable="true" parent="UIParent" enableMouse="true" hidden="false">
		<Size>
			<AbsDimension x="495" y="555"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Backdrop tile="true"
        bgFile="Interface\DialogFrame\UI-DialogBox-Background"
        edgeFile="Interface\DialogFrame\UI-DialogBox-Border">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer level="ARTWORK">
				<Texture name="StriLi_MainFrame_Header"
            file="Interface\DialogFrame\UI-DialogBox-Header">
					<Size>
						<AbsDimension x="400" y="64"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="12"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<FontString inherits="GameFontNormal" text="StriLi">
					<Anchors>
						<Anchor point="TOP" relativeTo="StriLi_MainFrame_Header">
							<Offset>
								<AbsDimension x="0" y="-13"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="StriLi_Chars" inherits="StriLi_ListTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="20" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="StriLi_MainNeed" inherits="StriLi_ListTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="111" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="StriLi_SecNeed" inherits="StriLi_ListTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="202" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="StriLi_TokenNeed" inherits="StriLi_ListTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="293" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="StriLi_Fails" inherits="StriLi_ListTemplate">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="384" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnMouseDown>StriLi_StartMoving(self);</OnMouseDown>
			<OnMouseUp>StriLi_StopMoving(self);</OnMouseUp>
			<OnDragStart></OnDragStart>
			<OnDragStop></OnDragStop>
		</Scripts>
	</Frame>
</Ui>
