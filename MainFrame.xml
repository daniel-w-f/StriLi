<Ui xmlns="http://www.blizzard.com/wow/ui/" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	
	<StatusBar name="StriLi_Template" virtual="true">
		<Size>
			<AbsDimension x="85" y="30"/>
		</Size>
		<BarTexture file="Interface\Tooltips\UI-Tooltip-Background"/>
		<BarColor r="1" g="1" b="1"/>
	</StatusBar>
	
	<Frame name="StriLi_Row_Template" virtual="true">
		<Size>
			<AbsDimension x="425" y="30"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT" relativePoint="TOPLEFT">
				<Offset>
					<AbsDimension x="35" y="-35"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Frames>
			<StatusBar name="StriLi_Name" inherits="StriLi_Template" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
			</StatusBar>
			<StatusBar name="StriLi_Main" inherits="StriLi_Template" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="85" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<BarColor r="0.8" g="0.8" b="0.8"/>
			</StatusBar>
			<StatusBar name="StriLi_Sec" inherits="StriLi_Template" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="170" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
			</StatusBar>
			<StatusBar name="StriLi_Token" inherits="StriLi_Template" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="255" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<BarColor r="0.8" g="0.8" b="0.8"/>
			</StatusBar>
			<StatusBar name="StriLi_Fail" inherits="StriLi_Template" useParentLevel="true">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="340" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
			</StatusBar>
		</Frames>
	</Frame>
	
	<Frame name="StriLi_MainFrame" toplevel="true" frameStrata="DIALOG" 
      movable="true" parent="UIParent" enableMouse="true" hidden="false">
		<Size>
			<AbsDimension x="495" y="555"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Backdrop tile="true"
        bgFile="Interface\DialogFrame\UI-DialogBox-Background"
        edgeFile="Interface\DialogFrame\UI-DialogBox-Border">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer level="ARTWORK">
				<Texture name="StriLi_MainFrame_Header"
            file="Interface\DialogFrame\UI-DialogBox-Header">
					<Size>
						<AbsDimension x="400" y="64"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="12"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<FontString inherits="GameFontNormal" text="StriLi">
					<Anchors>
						<Anchor point="TOP" relativeTo="StriLi_MainFrame_Header">
							<Offset>
								<AbsDimension x="0" y="-13"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
		</Frames>
		<Scripts>
			<OnMouseDown>StriLi_StartMoving(self);</OnMouseDown>
			<OnMouseUp>StriLi_StopMoving(self);</OnMouseUp>
			<OnDragStart></OnDragStart>
			<OnDragStop></OnDragStop>
			<OnLoad>StriLi_MainFrame_OnLoad();</OnLoad>
		</Scripts>
	</Frame>
</Ui>
